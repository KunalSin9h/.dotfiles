{
  "clangd.path": "/usr/bin/clangd",
  "clangd.fallbackFlags": [ 
      "-std=c++17",
      "-O2",
      "-Wall",
      "-Wextra",
      "-Wshadow",
      "-DLOCAL",
      "-fsanitize=undefined",
      "-fsanitize=address",
      "-Wformat=2",
      "-Wfloat-equal",
      "-Wconversion",
      "-Wcast-qual",
      "-Wcast-align",
      "-Wno-sign-conversion",
      "-D_GLIBCXX_DEBUG",
      "-D_GLIBCXX_DEBUG_PEDANTIC",
      "-fno-sanitize-recover=all",
      "-fstack-protector",
      "-D_FORTIFY_SOURCE=2"
  ],
  "cSpell.userWords": [
    "actix",
    "dealloc",
    "kunal",
    "runtimes",
    "singh",
    "suger",
    "trpc",
    "web"
  ]
}