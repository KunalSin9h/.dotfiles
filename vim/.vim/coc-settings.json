{
  "clangd.path": "/usr/bin/clangd",
  "inlayHint.enable": false,
  "clangd.fallbackFlags": [ 
      "-std=c++20",
      "-O2",
      "-Wall",
      "-Wextra",
      "-Wshadow",
      "-DLOCAL",
      "-fsanitize=undefined",
      "-fsanitize=address",
      "-Wformat=2",
      "-Wfloat-equal",
      "-Wconversion",
      "-Wcast-qual",
      "-Wcast-align",
      "-Wno-sign-conversion",
      "-D_GLIBCXX_DEBUG",
      "-D_GLIBCXX_DEBUG_PEDANTIC",
      "-fno-sanitize-recover=all",
      "-fstack-protector",
      "-D_FORTIFY_SOURCE=2"
  ],
  "cSpell.userWords": [
    "actix",
    "countl",
    "countr",
    "dealloc",
    "eoln",
    "gnu",
    "jsvm",
    "kiamol",
    "kubernetes",
    "kunal",
    "moneymap",
    "one",
    "pbds",
    "read",
    "rotr",
    "runtimes",
    "singh",
    "srishti",
    "suger",
    "trpc",
    "varidic",
    "vite",
    "web"
  ]
}